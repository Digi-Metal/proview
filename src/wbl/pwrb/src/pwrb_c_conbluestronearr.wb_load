!  pwrb_c_conbluestronearr.wb_load -- Defines the class ConBlueStrOneArr.
!
SObject pwrb:Class
  Object ConBlueStrOneArr $ClassDef 42 
    Body SysBody
      Attr Flags |= pwr_mClassDef_DevOnly
    EndBody
    Object DevBody $ObjBodyDef 2 
      Object PlcConnection $Buffer 1 
        Body SysBody
	  Attr PgmName = "PlcCon"
          Attr Class = pwr_eClass_PlcConnection
        EndBody
      EndObject
    EndObject
    Object GraphPlcCon $GraphPlcConnection
      Body SysBody
        Attr con_type = 18
        Attr arrows = 1
        Attr linewidth = 0.010
        Attr dashes = 0.
        Attr fillpattern = 1
        Attr curvature = 1
        Attr corners = 0.0
        Attr color = 1
      EndBody
    EndObject
  EndObject
EndSObject
