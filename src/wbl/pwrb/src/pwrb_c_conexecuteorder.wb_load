    Object DevBody $ObjBodyDef 2 
      Object PlcConnection $Buffer 1 
        Body SysBody
	  Attr PgmName = "PlcCon"
          Attr Class = pwr_eClass_PlcConnection
        EndBody
      EndObject
    EndObject
    Object GraphPlcCon $GraphPlcConnection
      Body SysBody
        Attr con_type = 28
        Attr arrows = 1
        Attr linewidth = 0.01
        Attr dashes = 0.
        Attr fillpattern = 1
        Attr curvature = 1
        Attr corners = 0.0
        Attr attributes = 149
      EndBody
    EndObject
  EndObject
