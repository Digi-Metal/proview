SObject pwrb:Type
!/**
!  @Version 1.0
!  @Group Types
!  Enumeration for classvolume storage.
!*/
  Object ClassVolumeDatabaseEnum $TypeDef 39
    Body SysBody
      Attr TypeRef = "pwrs:Type-$Enum"
      Attr PgmName = "ClassVolumeDatabaseEnum"
    EndBody
    !/**
    !  Store in wb_load text file.
    !*/
    Object WbLoad $Value
     Body SysBody
       Attr PgmName = "WbLoad"
       Attr Text = "WbLoad"
       Attr Value = 0
     EndBody
    EndObject
    !/**
    !  Store in Berkeley DB database.
    !*/
    Object BerkeleyDb $Value
     Body SysBody
       Attr PgmName = "BerkeleyDb"
       Attr Text = "BerkeleyDb"
       Attr Value = 1
     EndBody
    EndObject
    !/**
    !  Store in MySql database.
    !*/
    Object MySql $Value
     Body SysBody
       Attr PgmName = "MySql"
       Attr Text = "MySql"
       Attr Value = 2
     EndBody
    EndObject
  EndObject
EndSObject




