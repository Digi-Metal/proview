#
#  Script to export all classgraphs to java
#
#
#

function int process( string graph)
  open 'graph'
  printf( "Exporting %s...\n", graph);
  export java
endfunction

main()

  set javapath "$pwre_croot/java/jpwr/jopc/src"
  set javapackage "jopc"

  process( "pwr_c_ssab_safetyswitch");

endmain




