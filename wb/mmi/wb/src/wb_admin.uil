object  admin_window : XmMainWindow
{
  arguments
  {
    XmNx = 0;
    XmNy = 0;
!    XmNwidth = 450;
!    XmNheight = 400;
  };
  controls
  {
     XmMenuBar	admin_menu;
     XmForm  	form;
  };
};


object form : XmForm
{
   arguments
   {
     XmNtraversalOn = false;
     XmNwidth = 400;
     XmNheight = 600;
     XmNresizePolicy = XmRESIZE_NONE;
   };
