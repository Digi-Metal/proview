          {
            XmNactivateCallback = procedure wtt_activate_openvolume(wtt_ctx);
          };
	};
        XmCascadeButton 
        {
          arguments 
          {
	    XmNlabelString = compound_string("Open File");
	    XmNmnemonic         = keysym("F");
            XmNfontList	   = menu_font;
          };
